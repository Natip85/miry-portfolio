generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model GalleryImages {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  images String[]
} 

type Image {
  key                String
  name            String
  url                 String
  size               Int
  serverData  ServerData
}

type ServerData {
  uploadedBy String  
}